<table class='table'>
  <thead>
    <tr>
      <th>ユーザー名</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>

        <td><%= user.name %></td>
        <td><%= render 'public/relationships/follow',  user: user %></td>
        <td><%= link_to 'Show', user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
