<h2>検索結果</h2>
<table class="table table-hover table-inverse">
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @sceneries.each do |f| %>
        <tr>
          <td><%= link_to '投稿へ', scenery_path(f) %></td>
          <td><%= image_tag f.get_scenrery_image,  style: "width: 130px; align-self: flex-start;" %></td>
          <td><%= f.name %></td>
          <td><%= f.introduction %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>